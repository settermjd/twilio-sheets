{
  "name": "settermjd/twilio-gsheets-integration",
  "description": "This is a small project showing how to integrate OrderDesk, Twilio, and Google Sheets",
  "type": "project",
  "keywords": [
    "Google Docs",
    "Google Sheets",
    "Google",
    "OrderDesk",
    "PHP",
    "Slim Framework",
    "Twilio"
  ],
  "minimum-stability": "stable",
  "license": "MIT",
  "support": {
    "email": "matthew@matthewsetter.com"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.5",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-phpunit": "^2.0",
    "phpstan/phpstan-deprecation-rules": "^2.0",
    "laminas/laminas-coding-standard": "^3.1",
    "squizlabs/php_codesniffer": "^3.13"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "process-timeout": 600,
    "sort-packages": true
  },
  "require": {
    "google/apiclient": "^2.19",
    "php-di/slim-bridge": "^3.4",
    "slim/psr7": "^1.8",
    "slim/slim": "^4.15",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/App/"
    }
  }
}
